{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2017",
    "lib": ["ES2017", "DOM", "DOM.Iterable"],
    "moduleResolution": "bundler",
    "skipLibCheck": true
  }
}
